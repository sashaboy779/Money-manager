@inherits ModalComponent<CategoryModel>
@using CategoryModel = MoneyManagerUi.Data.Category.Category 

<div class="modal-window">
    <div class="mt-3 text-center">
        <p>@Resource.WantDeleteCategory</p>
    </div>
    <div class="d-flex justify-content-around">
        <button @onclick="InvokeModelFunctionAsync" type="submit" class="default-btn delete">@Resource.Yes</button>
        <button @onclick="Cancel" class="default-btn cancel">@Resource.No</button>
    </div>
</div>